class BaseClass {
    public:
        BaseClass(int value) {
            this.value = value;
        }

        int get_value() {
            return this.value;
        }

    protected:
        int value;
}

class DerivedClass : BaseClass {
    public:
        DerivedClass(int value) {
            this.value = 2 * value;
        }

        void set_value(int value) {
            this.value = value;
        }
}